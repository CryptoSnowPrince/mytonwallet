    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-teactn": "git+https://github.com/korenskoy/eslint-plugin-teactn#c2c39dd005d58c07c24c4361de804dce1c6261b5",
    "fantasticon": "^1.2.3",
